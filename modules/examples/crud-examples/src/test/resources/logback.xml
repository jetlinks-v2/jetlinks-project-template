<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <logger name="io.netty" level="warn"/>
    　
    <logger name="org.eclipse" level="warn"/>
    <logger name="org.hswebframework" level="debug"/>
    <logger name="org.springframework" level="warn"/>
    　 <logger name="org.jetlinks.supports.event" level="warn"/>
    　
    <logger name="org.hibernate" level="warn"/>
    　
    <logger name="reactor.netty" level="warn"/>
    　
    <logger name="org.apache" level="warn"/>
    　
    <logger name="io.r2dbc" level="warn"/>
    　
    <logger name="io.lettuce" level="warn"/>
    <logger name="org.testcontainers" level="warn"/>
    <logger name="reactor" level="warn"/>
    <logger name="com.github" level="warn"/>
    <logger name="org.jetlinks.pro.example" level="debug"/>
    <logger name="io.scalecube" level="error"/>

    　
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-4relative [%thread] %-5level %logger{35} - %msg %n</pattern>
        </encoder>
    </appender>

    <root level="debug">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>